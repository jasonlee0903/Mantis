{
  "type": "object",
  "properties": {
    "isMyview": {
      "type": "identifier",
      "selector": ".subtle",
      "control": "exists",
      "selectContains": "Assigned to Me"
    },
    "viewissuesLink": {
      "type": "action",
      "selector": "a",
      "control": "action.link",
      "selectContains": "View Issues"
    }
  },
  "connect": [
    {
      "match": [
        "isMyview"
      ],
      "action": "viewissuesLink"
    }
  ]
}
